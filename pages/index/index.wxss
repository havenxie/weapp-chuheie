/* 自定义弹框 */  
.model{  
  background:#fff;  
  position:fixed;  
  top:30%;  
  z-index:10000;  
  width:86%;  
  /* margin-left:7%;   */
  border-radius: 16rpx;  
}  
.modelTitle{  
  width:100%;  
  height:90rpx;  
  text-align:center;  
  line-height:100rpx;  
  border-bottom:1px solid #ECECED;  
  color:#222;  
  font-size:38rpx;  
  font-family:“微软雅黑”;  
}  
.modelBody{  
  color:#B1B1B1;  
  width:91%;  
  height:110rpx;  
  padding:30rpx;  
  font-size:35rpx;  
  line-height:60rpx;  
  padding-left:30rpx;  
  padding-top:40rpx;  
}  
.btns{  
  position:relative;  
  overflow:hidden;  
  border-top:1px solid #ECECED;  
}  
.model .btns button{  
  width:100%;  
  display:inline-block;  
  padding:0rpx;  
  background:#fff;  
  /* opacity: 0; */  
}  
.model .btns button::after {  
  border: none;  
}  
.model .btns .agree {  
  color:#21B922;  
}  
.mask{  
  width:100%;  
  height:100%;  
  background:rgba(0, 0, 0, .4);  
  position:fixed;  
  top:0;  
}  

.borderRed {
  border-color: red;
}

.modelPicker{  
 background:#fff;  
  position:fixed;  
  top:25%;  
  z-index:10000;  
  width:86%;  
  height: 50%;
  /* margin-left:7%;   */
  border-radius: 16rpx;  
  padding-top: 60rpx;
  font-size: 36rpx;
}  
.close{
  font-size: 56rpx;
  position: absolute;
  top: 0;
  right: 26rpx;
}
.selectArea{
  height: 80rpx;
  margin: 16rpx 40rpx 16rpx 16rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.selectTxt{
  padding: 0 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.selectBox{
  color:#222;  
  flex: 1;
  height: 76rpx;
  border: 1rpx solid #ECECED;
  border-radius: 16rpx;
  text-align: center;
  line-height: 76rpx;
  position: relative;
}
.xiala{
  font-size: 30rpx;
  color: rgba(91, 91, 97, 0.644);
  position: absolute;
  right: 16rpx;
}
.selectItems{
    position: absolute;
    z-index: 2;
    overflow-y: hidden;
    background: #fff;
}
.selectItem {
    height: 80rpx;
    line-height: 80rpx;
    border: 1rpx solid #ECECED;
    right: 16rpx;
}

.tips1 {
  color: red;
  font-size: 26rpx;
